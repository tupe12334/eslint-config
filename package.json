{
  "name": "@tupe12334/eslint-config",
  "version": "1.0.0",
  "description": "ESLint configuration package with TypeScript support",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:test": "eslint test/",
    "test": "node scripts/test-runner.js",
    "test:old": "npm run test:valid && npm run test:invalid",
    "test:valid": "eslint test/valid.tsx test/preact-test.tsx test/typescript-rules.ts",
    "test:invalid": "eslint test/invalid.tsx test/long-function.tsx test/jsx-extension-test.js",
    "test:hooks": "eslint test/react-hooks-rules.tsx",
    "test:imports": "eslint test/import-export-rules.ts",
    "test:edge": "eslint test/edge-cases.tsx",
    "test:performance": "eslint test/performance-test.tsx",
    "test:comprehensive": "node scripts/test-runner.js",
    "test:ci": "eslint . --ignore-pattern 'test/**' --ignore-pattern 'scripts/**' --max-warnings 0",
    "validate": "node scripts/validate-config.js",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:dry": "release-it --dry-run"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "javascript",
    "react",
    "preact",
    "tsx"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "release-it": "^19.0.4",
    "typescript-eslint": "^8.40.0"
  }
}
